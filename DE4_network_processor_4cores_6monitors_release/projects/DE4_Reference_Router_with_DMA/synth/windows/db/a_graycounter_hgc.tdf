--synthesis_resources = reg 6 
OPTIONS ALTERA_INTERNAL_OPTION = "suppress_da_rule_internal=S102;{-to parity9} POWER_UP_LEVEL=HIGH";

SUBDESIGN a_graycounter_hgc
( 
	aclr	:	input;
	clock	:	input;
	cnt_en	:	input;
	q[4..0]	:	output;
) 
VARIABLE 
	counter10a[4..0] : dffeas;
	parity9 : dffeas
		WITH (
			power_up = "high"
		);
	cntr_cout[4..0]	: WIRE;
	parity_cout	: WIRE;
	sclr	: NODE;
	updown	: NODE;

BEGIN 
	counter10a[].clk = clock;
	counter10a[].clrn = ( (! aclr), (! aclr), (! aclr), (! aclr), (! aclr));
	counter10a[].d = ( (counter10a[4].q $ cntr_cout[3..3]), (counter10a[3].q $ (counter10a[2].q & cntr_cout[2..2])), (counter10a[2].q $ (counter10a[1].q & cntr_cout[1..1])), (counter10a[1].q $ (counter10a[0].q & cntr_cout[0..0])), ((cnt_en & (counter10a[0].q $ (! parity_cout))) # ((! cnt_en) & counter10a[0].q)));
	counter10a[].sclr = sclr;
	parity9.clk = clock;
	parity9.d = ((cnt_en & ((((counter10a[0..0].q $ counter10a[1..1].q) $ counter10a[2..2].q) $ counter10a[3..3].q) $ counter10a[4..4].q)) # ((! cnt_en) & parity9.q));
	parity9.prn = (! aclr);
	parity9.sclr = sclr;
	cntr_cout[] = ( B"0", (cntr_cout[2..2] & (! counter10a[2].q)), (cntr_cout[1..1] & (! counter10a[1].q)), (cntr_cout[0..0] & (! counter10a[0].q)), (cnt_en & parity_cout));
	parity_cout = (((! parity9.q) $ (! updown)) & cnt_en);
	q[] = counter10a[].q;
	sclr = GND;
	updown = VCC;
END;
