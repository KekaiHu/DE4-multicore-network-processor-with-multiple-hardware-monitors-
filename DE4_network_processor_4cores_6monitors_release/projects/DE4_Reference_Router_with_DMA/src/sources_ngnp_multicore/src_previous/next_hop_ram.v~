//                                                                                            
//	8192 bytes, 32bit interface                                                               
                                                                                              
`timescale 1ns/1ps                                                                            
                                                                                              
                                                                                              
module next_hop_ram(clk, addr, data_in, data_out, we, en, reset);                             
input clk;                                                                                    
input [12:2] addr;                                                                            
input [31:0] data_in;                                                                         
output [31:0] data_out;                                                                       
input [3:0] we;                                                                               
input en;                                                                                     
input reset;                                                                                  
                                                                                              
wire [3:0] dip;                                                                               
                                                                                              
RAMB16_S9 ram0(                                                                               
                                                                                              
        .DO     (data_out[7:0]),                                                              
	.DOP    (),                                                                                 
	.ADDR   (addr[12:2]),                                                                       
	.CLK    (clk),                                                                              
	.DI     (data_in[7:0]),                                                                     
	.DIP    (dip[0]),                                                                           
	.EN     (en),                                                                               
	.SSR    (reset),                                                                            
	.WE     (we[0])                                                                             
                                                                                              
);                                                                                            
                                                                                              
